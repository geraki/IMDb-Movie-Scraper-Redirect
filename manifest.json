{
    "manifest_version": 3,
    "name": "IMDb Movie Scraper Redirect",
    "version": "1.0",
    "description": "Redirect IMDb movie pages to rdm.ayush.gg scraper.",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Go to Movie Scraper"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  